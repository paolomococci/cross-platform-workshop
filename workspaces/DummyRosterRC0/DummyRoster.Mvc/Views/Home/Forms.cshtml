@using DummyRoster.Common.EntityModel.Models
@model IEnumerable<Form>

<h2>@ViewData["Title"]</h2>

<table>
  <caption>@ViewData["Title"] registered in the management</caption>
  <thead>
    <tr>
      <th>Id</th>
      <th>Customer Id</th>
      <th>Carrier Id</th>
      <th>Employee Id</th>
      <th>Form Date</th>
      <th>Required Date</th>
    </tr>
  </thead>
  <tbody>
    @if (Model is not null)
    {
      @foreach (Form item in Model)
      {
        <tr>
          <td>
            @Html.DisplayFor(
              modelItem => item.Id
            )
          </td>
          <td>
            @Html.DisplayFor(
              modelItem => item.CustomerId
            )
          </td>
          <td>
            @Html.DisplayFor(
              modelItem => item.CarrierId
            )
          </td>
          <td>
            @Html.DisplayFor(
              modelItem => item.EmployeeId
            )
          </td>
          <td>
            @Html.DisplayFor(
              modelItem => item.FormDate
            )
          </td>
          <td>
            @Html.DisplayFor(
              modelItem => item.RequiredDate
            )
          </td>
        </tr>
      }
    }
  </tbody>
</table>
