@model DummyRoster.Common.EntityModel.Models.Form

<div class="card border-dark m-3" style="max-width: 20rem;">
  <img src="" class="card-img-top" alt="">
  <div class="card-header">
    ID: @Model.Id - Customer ID: @Model?.CustomerId
  </div>
  <div class="card-body text-dark">
    <h5 class="card-title">
      description and promised date
    </h5>
    <p class="card-text">
      @Model?.Description
    </p>
    <p class="card-text">
      @Model?.PromisedDate
    </p>
  </div>
  <div class="card-body">
    <a href="/" class="card-link">home</a>
  </div>
</div>
