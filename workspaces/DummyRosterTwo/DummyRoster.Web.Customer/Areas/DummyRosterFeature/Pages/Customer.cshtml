@page
@using DummyRoster.Common.EntityModel.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model DummyRoster.Web.Customer.DummyRosterFeature.Pages.CustomerModel

<div class="row">
    <h3 class="display-3">Customers</h3>
</div>

<div class="row">
    @foreach (Customer item in Model.customers)
    {
        <div class="col-sm-3">
            <partial name="_Customer" model="item" />
        </div>
    }
</div>
