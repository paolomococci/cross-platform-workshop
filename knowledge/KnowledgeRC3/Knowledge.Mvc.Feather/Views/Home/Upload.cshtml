@{
    ViewData["Title"] = "knowledge";
    ViewData["SubTitle"] = "upload";
}

<div class="card knowledge-card">
    <div class="card-body">
        <h2 class="card-title p-2">
            @ViewData["Title"]
        </h2>
        <h3 class="card-subtitle p-3 text-muted">
            @ViewData["SubTitle"]
        </h3>
        <p class="card-text p-2">Upload form.</p>
        <div class="card-footer">
            <form asp-action="Upload" enctype="multipart/form-data"></form>
        </div>
    </div>
</div>
