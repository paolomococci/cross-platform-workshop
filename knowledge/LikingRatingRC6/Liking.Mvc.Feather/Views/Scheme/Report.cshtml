@{
    ViewData["Title"] = "Liking";
}

<div class="uk-text-center uk-card-body uk-animation-scale-up uk-transform-origin-top-center">
    <h1 class="uk-heading-large uk-heading-divider">Report</h1>
</div>

<div class="uk-card uk-card-default uk-card-body uk-width-1-1">
    <h3 class="uk-card-title uk-margin-large-bottom">Processing report.</h3>
    <table class="uk-table uk-table-striped">
      <caption>metrics of model</caption>
      <thead>
        <tr>
            <th>Metrics</th>
            <th>Values</th>
        </tr>
    </thead>
    <tbody>
      <tr>
        <td>Accuracy</td>
        <td>@ViewBag.Models.Accuracy</td>
      </tr>
      <tr>
        <td>Area Under Roc Curve</td>
        <td>@ViewBag.Models.AreaUnderRocCurve</td>
      </tr>
      <tr>
        <td>Area Under Precision Recall Curve</td>
        <td>@ViewBag.Models.AreaUnderPrecisionRecallCurve</td>
      </tr>
      <tr>
        <td>F1 Score</td>
        <td>@ViewBag.Models.F1Score</td>
      </tr>
      <tr>
        <td>Log Loss</td>
        <td>@ViewBag.Models.LogLoss</td>
      </tr>
      <tr>
        <td>Log Loss Reduction</td>
        <td>@ViewBag.Models.LogLossReduction</td>
      </tr>
      <tr>
        <td>Positive Precision</td>
        <td>@ViewBag.Models.PositivePrecision</td>
      </tr>
      <tr>
        <td>Positive Recall</td>
        <td>@ViewBag.Models.PositiveRecall</td>
      </tr>
      <tr>
        <td>Negative Precision</td>
        <td>@ViewBag.Models.NegativePrecision</td>
      </tr>
      <tr>
        <td>Negative Recall</td>
        <td>@ViewBag.Models.NegativeRecall</td>
      </tr>
    </tbody>
    </table>
</div>
