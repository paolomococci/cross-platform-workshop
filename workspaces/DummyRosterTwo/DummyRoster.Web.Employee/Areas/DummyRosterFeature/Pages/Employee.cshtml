@page
@using DummyRoster.Common.EntityModel.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model DummyRoster.Web.Employee.DummyRosterFeature.Pages.EmployeeModel

<div class="row">
    <h3 class="display-3">Employees</h3>
</div>

<div class="row">
    @foreach (Employee item in Model.employees)
    {
        <div class="col-sm-3">
            <partial name="_Employee" model="item" />
        </div>
    }
</div>
