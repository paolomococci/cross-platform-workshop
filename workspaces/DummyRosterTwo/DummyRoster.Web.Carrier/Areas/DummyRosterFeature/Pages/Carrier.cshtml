@page
@using DummyRoster.Common.EntityModel.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model DummyRoster.Web.Carrier.DummyRosterFeature.Pages.CarrierModel

<div class="row">
    <h3 class="display-3">Carriers</h3>
</div>

<div class="row">
    @foreach (Carrier item in Model.carriers)
    {
        <div class="col-sm-3">
            <partial name="_Carrier" model="item" />
        </div>
    }
</div>
