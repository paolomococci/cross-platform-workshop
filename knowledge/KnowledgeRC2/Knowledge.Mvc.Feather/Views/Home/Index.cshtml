@{
    ViewData["Title"] = "knowledge";
    ViewData["SubTitle"] = "index";
}

<div class="card knowledge-card">
    <div class="card-body">
        <h2 class="card-title p-2">
            @ViewData["Title"]
        </h2>
        <h3 class="card-subtitle p-3 text-muted">
            @ViewData["SubTitle"]
        </h3>
        <p class="card-text p-2">Link to call up the upload form.</p>
        <div class="card-footer">
            <a asp-area="" asp-controller="Home" asp-action="Upload" class="m-5 card-link">
                <i title="upload" class="bi bi-upload" style="font-size: 5rem;"></i>
            </a>
        </div>
    </div>
</div>
