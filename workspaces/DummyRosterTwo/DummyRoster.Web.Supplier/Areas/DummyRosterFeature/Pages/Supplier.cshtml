@page
@using DummyRoster.Common.EntityModel.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model DummyRoster.Web.Supplier.DummyRosterFeature.Pages.SupplierModel

<div class="row">
    <h3 class="display-3">Suppliers</h3>
</div>

<div class="row">
    @foreach (Supplier item in Model.suppliers)
    {
        <div class="col-sm-3">
            <partial name="_Supplier" model="item" />
        </div>
    }
</div>
