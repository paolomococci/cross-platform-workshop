@{
    ViewData["Title"] = "PivotUp";
}

<div class="text-center">
    <h1 class="display-4">Upload</h1>
    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="Upload">
      <span>Select</span>
      <input type="file" name="items" multiple>
      <input type="submit" value="Upload">
      <br>
      <span>@Html.Raw(ViewBag.Message)</span>
    </form>
</div>
