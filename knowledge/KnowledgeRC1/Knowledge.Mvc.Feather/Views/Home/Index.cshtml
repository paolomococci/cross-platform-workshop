@{
    ViewData["Title"] = "knowledge";
    ViewData["SubTitle"] = "index";
}

<div class="card" style="width: 30rem;">
    <div class="card-body">
        <h2 class="card-title">
            @ViewData["Title"]
        </h2>
        <h3 class="card-subtitle mb-2 text-muted">
            @ViewData["SubTitle"]
        </h3>
        <p class="card-text">item list:</p>
        <ul class="list-group list-group-flush">
            <li class="list-group-item">item one</li>
            <li class="list-group-item">item two</li>
            <li class="list-group-item">item three</li>
            <li class="list-group-item">item four</li>
            <li class="list-group-item">item five</li>
        </ul>
        <a href="#" class="card-link">link one</a>
        <a href="#" class="card-link">link two</a>
    </div>
</div>
