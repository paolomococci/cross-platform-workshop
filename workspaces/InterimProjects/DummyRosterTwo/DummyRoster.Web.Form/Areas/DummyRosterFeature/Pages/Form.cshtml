@page
@using DummyRoster.Common.EntityModel.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model DummyRoster.Web.Form.DummyRosterFeature.Pages.FormModel

<div class="row">
    <h3 class="display-3">Forms</h3>
</div>

<div class="row">
    @foreach (Form item in Model.forms)
    {
        <div class="col-sm-3">
            <partial name="_Form" model="item" />
        </div>
    }
</div>
